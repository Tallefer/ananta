<?xml version="1.0" encoding="UTF-8"?>
<commodities>
  <shipparts>
    <!-- commodity type: SHIPPARTS (special type) -->
    <hulls>
      <!-- ship hulls -->
      <samplehull1>
        <!-- Hull ID -->
        <name>Nadia</name>
        <!-- hull name -->
        <unit>piece</unit>
        <!-- unit of measurement (piece or kg) -->
        <mass>12000</mass>
        <!-- empty hull mass in kg -->
        <avgprice>1200</avgprice>
        <!-- average price -->
        <volume>600</volume>
        <!-- hull external volume in m^3 (note: not INTERNAL volume!) -->
        <image>ship1.png</image>
        <!-- the pic representing this hull -->
        <scale>0.3</scale>
        <!-- the scale of the visual image (1 = full resolution of the image) -->
        <size>20</size>
        <!-- the diameter of the hull in meters (for radar blip size, rotational physics and rough collision detection) -->
        <thrusterpos>20</thrusterpos>
        <!-- distance (in meters) of the rotation thrusters from the center of the mass -->
        <maxspeed>100</maxspeed>
        <!-- the maximum speed allowed for this hull in m/s (1 standard pixel equals 1m) -->
        <maxrotspd>180</maxrotspd>
        <!-- the maximum rotational speed (deg/s) allowed for this hull -->
        <reverser>0.6</reverser>
        <!-- the portion of the main engine thrust that can be reversed -->
        <slots>
          <leftrotthruster>
            <!-- slot ID -->
            <type>rotthruster</type>
            <!-- slot type -->
            <volume>1</volume>
            <!-- the volume of the slot in m^3 -->
            <exposure>left</exposure>
            <!-- exposure to space: left, right, nose, tail. Remove if no exposure -->
            <location>external</location>
            <!-- internal/external, affects received damage -->
          </leftrotthruster>
          <rightrotthruster>
            <!-- slot ID -->
            <type>rotthruster</type>
            <!-- slot type -->
            <volume>1</volume>
            <!-- the volume of the slot in m^3 -->
            <exposure>right</exposure>
            <!-- exposure to space: left, right, nose, tail. Remove if no exposure -->
            <location>external</location>
            <!-- internal/external, affects received damage -->
          </rightrotthruster>
          <transright>
            <!-- slot ID -->
            <type>thruster</type>
            <volume>1</volume>
            <exposure>right</exposure>
            <!-- exposure to the right -> movement to the left -->
            <location>external</location>
            <!-- internal/external, affects received damage -->
          </transright>
          <transleft>
            <!-- slot ID -->
            <type>thruster</type>
            <volume>1</volume>
            <exposure>left</exposure>
            <!-- exposure to the left -> movement to the right -->
            <location>external</location>
            <!-- internal/external, affects received damage -->
          </transleft>
          <transnose>
            <!-- slot ID -->
            <type>thruster</type>
            <volume>1</volume>
            <exposure>nose</exposure>
            <!-- exposure to the front -> movement backward -->
            <location>external</location>
            <!-- internal/external, affects received damage -->
          </transnose>
          <transtail>
            <!-- slot ID -->
            <type>thruster</type>
            <volume>1</volume>
            <exposure>tail</exposure>
            <!-- exposure to the back -> movement forward -->
            <location>external</location>
            <!-- internal/external, affects received damage -->
          </transtail>
          <engineslot1>
            <!-- slot ID -->
            <type>engine</type>
            <volume>5</volume>
            <!-- the volume of the slot in m^3 -->
            <exposure>tail</exposure>
            <!-- main engines always point behind... other directions have no effect -->
            <location>external</location>
            <!-- internal/external, affects received damage -->
          </engineslot1>
          <equipmentslot1>
            <!-- slot ID -->
            <type>equipment</type>
            <volume>100</volume>
            <!-- volume of the slot in m^3 -->
            <location>internal</location>
            <!-- internal/external, affects received damage -->
          </equipmentslot1>
        </slots>
      </samplehull1>
