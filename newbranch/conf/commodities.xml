            <!-- internal/external, affects received damage -->
          </transtail>
          <engineslot1>
            <!-- slot ID -->
            <type>engine</type>
            <volume>5</volume>
            <!-- the volume of the slot in m^3 -->
            <exposure>tail</exposure>
            <!-- main engines always point behind... other directions have no effect -->
            <location>external</location>
            <!-- internal/external, affects received damage -->
          </engineslot1>
          <equipmentslot1>
            <!-- slot ID -->
            <type>equipment</type>
            <volume>100</volume>
            <!-- volume of the slot in m^3 -->
            <location>internal</location>
            <!-- internal/external, affects received damage -->
          </equipmentslot1>
        </slots>
      </samplehull2>
    </hulls>
    <propulsion>
      <!-- all propulsion equipment -->
      <trilliumengine1>
